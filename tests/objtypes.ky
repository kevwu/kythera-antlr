let myObj = {
    a = 99
    b = "beep"
}

myObj.a
myObj.b

typeof myObj

name myObjType obj{
    int a
    str b
}

let myCastedObj = myObj as myObjType
typeof myCastedObj

typeof myCastedObj == typeof (myObj as myObjType)

let mySpecificFn = fn(myObjType object): null {
    object.a
    object.b
    return 10
}

mySpecificFn(myObj as myObjType)
mySpecificFn(myCastedObj)